<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.kotlin.android.home">

    <application
        android:name="debug.HomeApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_module"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_module_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:targetApi="n">

        <activity
            android:name=".ui.main.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:theme="@style/AppThemeNoFitsSystemWindows"
            android:name=".ui.toplist.TopListDetailActivity"
            />
        <activity
            android:theme="@style/AppThemeNoFitsSystemWindows"
            android:name=".ui.toplist.TopListGameDetailActivity"
            />
        <activity
            android:name=".ui.toplist.TopListGameActivity"
            />
    </application>
</manifest>