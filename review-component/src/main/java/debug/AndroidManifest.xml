<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.kotlin.android.review.component">

    <application
        android:name="debug.ReviewApp"
        android:allowBackup="false"
        android:icon="@mipmap/ic_module"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_module_round"
        android:supportsRtl="false"
        android:theme="@style/AppTheme"
        tools:replace="android:name"
        tools:targetApi="n">
        <activity android:name=".item.ui.detail.ReviewDetailActivity"
            android:screenOrientation="fullSensor"/>

        <activity android:name="debug.main.ReviewDebugActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>