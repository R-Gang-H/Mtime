<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.kotlin.android.community">

    <application
        android:name="debug.CommunityApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_module"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_module_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:targetApi="n">

        <activity
            android:name="debug.main.CommunityActivity"
            android:screenOrientation="fullSensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.person.center.CommunityPersonActivity"
            android:screenOrientation="fullSensor" />
        <activity
            android:name=".ui.person.myfriend.CommunityMyFriendActivity"
            android:screenOrientation="fullSensor" />
        <activity android:name=".ui.person.wantsee.WantSeeActivity"/>
    </application>
</manifest>