<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.kotlin.android.community.family.component">

    <application
        android:name="debug.FamilyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_module"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_module_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:targetApi="n">

        <activity
            android:name="debug.main.FamilyActivity"
            android:screenOrientation="fullSensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.clazz.FamilyClassListActivity"/>
        <activity
            android:theme="@style/AppThemeNoFitsSystemWindows"
            android:name=".ui.details.FamilyDetailActivity"/>
        <activity
            android:name=".ui.manage.FamilyCreateActivity" />
        <activity
            android:name=".ui.manage.FamilyEditInfoActivity" />
        <activity
            android:name=".ui.manage.FamilyPermissionActivity" />
        <activity
            android:name=".ui.manage.FamilyCategoryActivity" />
        <activity
            android:name=".ui.manage.FamilyAdminActivity" />
        <activity
            android:name=".ui.manage.FamilyAddAdminActivity" />
        <activity
            android:name=".ui.manage.FamilyMemberActivity" />
        <activity
            android:name=".ui.manage.FamilyMemberManageActivity" />
    </application>
</manifest>